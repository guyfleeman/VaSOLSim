<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="vssRemote">
        <xs:complexType>
            <xs:sequence>

                <!-- identification info -->
                <xs:element name="information">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="name" type="xs:string"/>
                            <xs:element name="remote" type="xs:string"/>
                            <xs:element name="forceSSL" type="xs:string"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>